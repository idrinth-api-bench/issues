{
  "name": "@idrinth/api-bench/history-website",
  "private": true,
  "version": "2.7.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig tsconfig.json",
    "lint-md": "markdownlint *.md",
    "lint": "eslint . --ext ts,svelte --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint . --ext ts,svelte --report-unused-disable-directives --max-warnings 0 --fix",
    "language": "itlfy generate --verbatimModuleSyntax --split",
    "postinstall": "npm run language"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "svelte": "^4.2.11",
    "svelte-check": "^3.6.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.35.1",
    "markdownlint-cli": "^0.39.0",
    "@idrinth/typescript-language-from-yaml": "^1.0.1"
  },
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true
}
